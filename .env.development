# Development Environment Variables Template
# Copy this file to .env.development.local and fill in actual values

# Database Configuration  
MONGODB_URI=mongodb://localhost:27017/your_dev_database

# Server Configuration
PORT=5000
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_dev_jwt_secret_here

# Frontend URLs
CLIENT_BASE_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3000
CORS_ORIGIN=http://localhost:3000

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Session Secret
SESSION_SECRET=your_dev_session_secret_here

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloudinary_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Razorpay Configuration (Test Keys)
RAZORPAY_KEY_ID=your_test_razorpay_key_id
RAZORPAY_KEY_SECRET=your_test_razorpay_secret
